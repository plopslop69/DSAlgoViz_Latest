<div class="col col_custom">
  <div class="row g-0">
    <div class="col col-md-6">
      <img
        src="/images/choosealgo/<%= thumb %>"
        class="img-fluid"
        alt="<%= title %>"
        id="img3"
      />
    </div>
    <div class="col-md-6">
      <div class="card-body">
        <h5 class="card-title"><%= title %></h5>
        <p class="card-text">
          <%= text %>

          <span id="dots">...</span>

          <button
            onclick="location.href='/resources/<%= resource %>'"
            id="myBtn"
          >
            Read more
          </button>
        </p>
        <button class="btn" type="submit" id="btn" value="<%= id %>">
          Go visualize
        </button>
      </div>
    </div>
  </div>
</div>
